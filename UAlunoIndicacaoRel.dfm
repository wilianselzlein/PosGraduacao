  object QGerencial: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      
        'select ALUINDICACAO,ALUINDICACAOPAGO,CAST(COUNT(ALUCOD) AS INTEG' +
        'ER) AS COUNTA, SUM(ALUINDICACAOVALOR),'
      
        'CAST(((COUNT(ALUCOD) * 100) / (SELECT COUNT(ALUCOD) FROM TALUNO)' +
        ') AS FLOAT) AS PERC'
      'from TALUNO'
      'WHERE ALUCOD > 0'
      'GROUP BY ALUINDICACAO,ALUINDICACAOPAGO'
      'ORDER BY ALUINDICACAO,ALUINDICACAOPAGO')
    SQLConnection = DM.SQLConnection
    Left = 24
