procedure TDm.DataModuleCreate(Sender: TObject);
var
    arq: TextFile;
    linha: string;
    i: integer;
begin
     if fileexists('c:\caminho.txt') then
     begin
          AssignFile(arq, 'C:\caminho.txt');
          Reset(arq);
          ReadLn(arq, linha);
          if uppercase(copy(linha, 1, 1)) <> 'C' then
          begin
               if FileExists(ExtractFilePath(Application.ExeName) + 'banco.fdb') then
               begin
                    if application.messagebox('Existe um banco de dados local, deseja acessa-lo? Caso contrrio ser acessado o banco de dados da rede', 'Ateno', mb_yesno + MB_ICONQUESTION) = mrno then
                    begin
                         sqlconnection.Params.Values['Database'] := linha;
                         local := false;
                    end;
               end
               else
                    sqlconnection.Params.Values['Database'] := linha;
          end
          else
               sqlconnection.Params.Values['Database'] := linha;
          closefile(arq);
     end;

     //Carregando(True);
     try
          SQLConnection.connected := true;
          Carregando(False);
     except
          on E: Exception do
          begin
               Carregando(False);
               showmessage('Impossvel conectar com o banco de dados!' + #13 + #13 + E.Message);
               if fileexists('c:\caminho.txt') then
                    ShellExecute(GetDesktopWindow, 'open', pchar('c:\caminho.txt'), nil, nil, sw_ShowNormal);
               application.terminate;
          end;
     end;

     for i := 0 to DM.ComponentCount - 1 do
        if DM.Components[i] is TClientDataSet then
            TClientDataSet(DM.Components[i]).OnReconcileError := dm.CDSCidadeReconcileError;

     Carregando(True);
     atualizar_sistema;
     ThreadExecSql(dm.cdsusuario);
end;

