procedure TDm.CDSAvisoAfterScroll(DataSet: TDataSet);
begin
     CDSAvisoCurso.Close;
     QAvisoCurso.Params.ParamByName('AVICURAVISO').AsInteger := CDSAvisoAviCod.AsInteger; 
     CDSAvisoCurso.Open;
     if FAviso <> nil then
     begin
          FAviso.chkCurso.Items.Clear;
          if not CDSGrupoCurso.Active then
               CDSGrupoCurso.Open;
          //CDSGrupoCurso.AfterScroll := nil;
          CDSGrupoCurso.First;
          while not CDSGrupoCurso.Eof do
          begin
               FAviso.chkCurso.Items.Add(FormatFloat('0000',CDSGrupoCursoGRUCOD.asInteger) + '-' + CDSGrupoCursoGRUNOME.asString);
               if CDSAvisoCurso.locate('AVICURCURSO', CDSGrupoCursoGRUCOD.AsINteger, [loPartialKey, loCaseInsensitive]) then
                    FAviso.chkCurso.Checked[FAviso.chkCurso.Items.Count - 1] := True;
               CDSGrupoCurso.Next;
          end;
          //CDSGrupoCurso.AfterScroll := CDSGrupoCursoAfterScroll;
     end;
end;

